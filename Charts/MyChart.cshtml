@{
    var db = Database.Open("Cafeen") ;
    var dbdata = db.Query("SELECT ProductName, Stock FROM Products"); 
    var MyChart = new Chart(width: 600, height: 300, theme: ChartTheme.Blue) 
            .AddTitle("Product Stock") 
            .DataBindTable(dataSource: dbdata, xField: "ProductName") 
            .Write();   
}

